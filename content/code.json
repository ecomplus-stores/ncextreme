  "html_body": "<a href=\"https://wa.me/5562998326146\" style=\"position:fixed;width:60px;height:60px;bottom:40px;background-color:#000000;color:#FFF;border-radius:50px;text-align:center;font-size:30px;box-shadow: 1px 1px 2px #888;z-index:1000;right:40px;\" target=\"_blank\">\n<i style=\"margin-top:16px\" class=\"fab fa-whatsapp\"></i>\n</a>\n\n<script>\n  document.addEventListener(\"DOMContentLoaded\", function () {\n    if (/camiseta-oversized/.test(window.location.pathname)) {\n      restriced();\n    }\n  });\n  \n  function restriced() {\n      const buttonBuy = document.querySelector('.btn.btn-lg.btn-primary')\n      \n      if (!buttonBuy) {\n        console.warn(\"Botão COMPRAR não encontrado.\");\n        return;\n      }\n      \n      const dateFinal = new Date(\"2025-07-26T13:00:00Z\"); // 13h UTC\n\n      function update() {\n          const now = new Date();\n        \n          if (now < dateFinal) {\n             buttonBuy.style.backgroundColor = '#c01818';\n             const rest = dateFinal - now\n             \n             const dias = Math.floor(rest / (1000 * 60 * 60 * 24));\n             const horas = Math.floor((rest / (1000 * 60 * 60)) % 24);\n             const minutos = Math.floor((rest / (1000 * 60)) % 60);\n             const segundos = Math.floor((rest / 1000) % 60);\n            \n             buttonBuy.innerText = `${horas}:${minutos}:${segundos}`\n\n             buttonBuy.disabled = true\n          } else {\n              return console.log(\"Botão voltou ao normal!\")\n          }\n      }\n    \n      update();\n      setInterval(update, 1000);\n  }\n  \n  document.addEventListener(\"DOMContentLoaded\", function() {\n\n      \n    const elements = document.querySelectorAll('.header__category');\n\n    elements.forEach(function(el) {\n      if (el.textContent.trim() === \"HENRIQUE MOREIRA\") {\n        //el.style.color = \"#c01818\";\n        el.style.display = \"none\";\n      }\n    });\n  });\n</script>"
